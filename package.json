{
  "name": "waypn-bot-lite",
  "version": "1.0.0",
  "description": "Telegram bot для оплаты VPN подписки через Tribute",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "webhook": "node webhook.js",
    "dev": "nodemon index.js",
    "dev:webhook": "nodemon webhook.js",
    "test:webhook": "node test-webhook.js",
    "start:all": "concurrently \"npm run start\" \"npm run webhook\"",
    "start:prod": "./start.sh",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:webhook\""
  },
  "keywords": ["telegram", "bot", "vpn", "tribute"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "node-telegram-bot-api": "^0.64.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "body-parser": "^1.20.2",
    "axios": "^1.6.0",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
} 